
window.onload = function() {
	document.getElementById("ok").onclick = okClick;
} // 이 안에 있는놈은 다 읽어올때까지 기다렸다가 해라.. 대신 딱 한번만 호출된다. 그러면 매번 이 함수를 호출해야 하는가? 한번만 호출해야 메모리 절약됨   


function okClick() {
	alert("ok clicked!");
}
