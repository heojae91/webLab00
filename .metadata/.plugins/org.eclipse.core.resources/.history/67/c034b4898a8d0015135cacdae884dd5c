window.onload = function() {
	document.getElementById("ok").onclick = function() {
		var sn = $("contect")["sir_name"];
		$("p1").innerHTML = $F(sn);
		
		if ($F("gn")).length > 7) {
			$("gn").clear();
		}
	};
};   

